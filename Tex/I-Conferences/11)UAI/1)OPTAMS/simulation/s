dim = 1;
B   = ones(dim,dim);
x   = ones(dim,1);
init = 5;

iter = 20;
eta  = 0.1;

x = init;
for t=1:iter
	x = x  - eta * B*x
end

%{
u = v = init;
for t=1:iter
	u = u  - eta * B*v;
	v = u  - eta * B*v;
end 

y = z = init;
GradMa = B*y;

for t=1:iter
	y = y  - eta * B*z;
	GradMa = [GradMa By];
	optm = ex(GradMa);
	z = y  - eta * B*z;
end 
%}
